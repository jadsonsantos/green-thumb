{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["plugin:prettier/recommended", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "comma-dangle": "off"
  }
}
